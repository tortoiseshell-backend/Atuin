{
  "presets": [
    ["@babel/preset-env", { "useBuiltIns": "usage", "corejs": 3 }],
    ["@babel/preset-react", { "useBuiltIns": true }]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }],
    "@babel/plugin-transform-modules-commonjs"
  ]
}
